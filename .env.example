# Webapp Environment Variables Template
# Copy this file to .env and fill in your actual values

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
# GOOGLE_REDIRECT_URI is optional - auto-constructed from request host if not set
# GOOGLE_REDIRECT_URI=http://localhost:7272/api/auth/callback

# JWT Secret
JWT_SECRET_KEY=your-secret-key-$(date +%s)

# Database Credentials
DB_USER=phi_user
DB_PASSWORD=your-postgres-password
DB_NAME=phi_here

# Neo4j Credentials
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password
NEO4J_DATABASE=neo4j

# OpenAI API
OPENAI_API_KEY=your-openai-api-key
